{
  "name": "langaly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.0.0-alpha.4",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "husky": "1.3.0",
    "lint-staged": "8.1.0",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
